@using BikeShop.Models.Item
@model IEnumerable<ItemListViewModel>

@{
    ViewData["Title"] = "Items";
}

<h1>Item List</h1>

<table class="table">

    <thead>
        <tr>
            <th>Id</th>
        </tr>
    </thead>
    <tbody>

@foreach (var item in Model) {

        <tr>
            <td>@item.ItemId</td>
        </tr>
}
    </tbody>
</table>
